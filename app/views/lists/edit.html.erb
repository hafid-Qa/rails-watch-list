<div class="container mt-5">
  <div class="p-5 mt-4 m-4 bg-light rounded-3 text-dark">
    <%= simple_form_for [@list,@bookmark] do |f| %>
      <%= f.input :comment %>
      <%= f.association :movie %>
      <%= f.button :submit,"Add movie",  class: "btn btn-success" %>
    <% end %>
  </div>
</div>
